<div class="form-group">
    <label for="exampleSelectRounded0">Statut</label>
    <select class="custom-select rounded-0" ng-model="chosenFilters.statut">
        {% for d in statut %}
            <option value="{{ d.id }}">{{ d.lib }}</option>
        {% endfor %}
    </select>
</div>

