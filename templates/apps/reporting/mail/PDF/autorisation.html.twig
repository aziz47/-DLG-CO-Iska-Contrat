{% extends 'apps/reporting/mail/PDF/base.html.twig' %}
{% block body %}
    <table id="example1" class="table table-bordered table-hover">
        <thead>
        <tr>
            {% for h in headers %}
                <th>{{ h }}</th>
            {% endfor %}
        </tr>
        </thead>
        <tbody>
        {% for autorisation in filtersData %}
            <tr>
                <td colspan="0">{{autorisation.id}}</td>
                <td colspan="0">{{autorisation.objet}}</td>
                <td colspan="0">{{autorisation.createdBy}}</td>
                <td colspan="0">{{autorisation.currentState}}</td>
                <td colspan="0">{{autorisation.createdAt}}</td>
            </tr>
        {% endfor %}
        </tbody>
        <tfoot>
        <tr>
            {% for h in headers %}
                <th>{{ h }}</th>
            {% endfor %}
        </tr>
        </tfoot>
    </table>
{% endblock %}